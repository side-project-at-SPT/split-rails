<div id="<%= dom_id room %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= room.name %>
    <hr>
    <strong class="block font-medium mb-1">Player:</strong>
    <% room.players.each do |player| %>
      <p class="my-5 list-item ml-5">
        <%= player.name %>
      </p>
    <% end %>
  </p>

</div>
